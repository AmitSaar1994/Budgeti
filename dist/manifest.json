{
    "manifest_version": 3,
    
    "name": "Budgeti",
    "description": "Manage your budget",
    "version": "1.0",

    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
      },

      "permissions": [
        "activeTab",
        "background",
        "scripting",
        "tabs", "http://*/*", "https://*/*"



      ]
  }